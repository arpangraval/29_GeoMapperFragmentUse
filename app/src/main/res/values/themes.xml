<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GeoMapper" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/Primarycolor</item>
        <item name="colorPrimaryDark">@color/fontcolor</item>
        <item name="colorAccent">@color/iconcolor</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="FullScreenTheme" parent="Theme.GeoMapper">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>